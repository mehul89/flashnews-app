{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  